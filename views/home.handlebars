
{{#each data}}
  <div class="card">
    <div class="card-header">
      <h4 class="data-title">{{this.articles.title}}</h4>
    </div>
    <div class="card-content">
      <p>{{this.link}}</p>
    </div>
    <div class="card-footer">
      <a href="{{this.link}}">Click Here To Visit The Article</a>
    </div>
  </div>
{{/each}}